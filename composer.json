{
    "name": "honda/url-resolver",
    "description": "Resolve routes and urls with one function.",
    "keywords": [
        "laravel",
        "url",
        "resolver",
        "url-resolver"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "FÃ©lix Dorn",
            "email": "contact@felixdorn.fr",
            "homepage": "https://felixdorn.fr"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/contracts": "^8|^9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "orchestra/testbench": "^v6.25|^7.7",
        "pestphp/pest": "^1.0.5",
        "phpstan/phpstan": "^1",
        "symfony/var-dumper": "^5|^6.1"
    },
    "autoload": {
        "psr-4": {
            "Felix\\UrlResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Felix\\UrlResolver\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix -v",
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:types": "phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
